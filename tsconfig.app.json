{
  "extends": "./tsconfig.json",
  
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "allowJs": true,
    "types": [],
  
  },
  "angularCompilerOptions": {
    "enableIvy": false,
    "skipTemplateCodegen": true,
      "skipMetadataEmit": false,
      "strictMetadataEmit": true,
      "annotationsAs": "decorators"
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
